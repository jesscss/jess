{
  "name": "@jesscss/css-parser",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.0.0-alpha.1",
  "description": "Jess CSS base parser",
  "main": "lib/index",
  "types": "lib/index",
  "files": [
    "lib"
  ],
  "scripts": {
    "ci": "pnpm build && pnpm test",
    "prepublishOnly": "pnpm build",
    "build": "pnpm compile",
    "compile": "tsc -b tsconfig.build.json",
    "dev": "tsc -b tsconfig.build.json -w",
    "test": "cross-env TEST=true vitest",
    "tes:coverage": "cross-env TEST=true vitest --coverage",
    "lint:fix": "eslint --fix '**/*.{js,ts}'",
    "lint": "eslint '**/*.{js,ts}'"
  },
  "dependencies": {
    "@jesscss/core": "workspace:^2.0.0-alpha.1",
    "chevrotain": "^11.0.3",
    "chevrotain-allstar": "~0.3.0",
    "lodash-es": "~4.17.21",
    "xregexp": "^5.1.1"
  },
  "devDependencies": {
    "@less/test-data": "^4.1.0",
    "@types/lodash-es": "~4.17.8",
    "@types/xregexp": "^4.4.0"
  },
  "author": "Matthew Dean <matthew-dean@users.noreply.github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jesscss/jess/issues"
  },
  "homepage": "https://github.com/jesscss/jess#readme"
}
