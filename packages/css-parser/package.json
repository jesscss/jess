{
  "name": "@jesscss/css-parser",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.0.0-alpha.1",
  "description": "Jess CSS parser",
  "main": "lib/index",
  "types": "lib/index",
  "type": "module",
  "files": [
    "lib"
  ],
  "scripts": {
    "generate": "node generate.mjs",
    "ci": "pnpm build && pnpm test",
    "prepublishOnly": "pnpm build",
    "build": "pnpm clean && pnpm compile",
    "clean": "shx rm -rf ./lib tsconfig.tsbuildinfo",
    "compile": "tsc -b tsconfig.json",
    "dev": "tsc -b tsconfig.json -w",
    "test": "NODE_OPTIONS=\"--loader ts-node/esm\" mocha test/**/*.ts"
  },
  "author": "Matthew Dean",
  "license": "MIT",
  "devDependencies": {
    "@jesscss/grammar": "workspace:^2.0.0-alpha.1",
    "@less/test-data": "^4.1.0",
    "shelljs": "^0.8.5"
  },
  "dependencies": {
    "antlr4": "4.12.0"
  }
}
