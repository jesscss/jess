@mixin domeMixin {
  one: 1;
  two: 2;
}
@mixin domeMixin () {
  one: 1;
  two: 2;
}
@mixin domeMixin (one, two) {}
@mixin domeMixin (one, two: 2) {}
@mixin domeMixin (one: 1, two: 2) {}

@include domeMixin();
@include domeMixin(1);
@include domeMixin(1, 2);

@rules someRules() {
  .foo {
    bar: foo;
  }
}