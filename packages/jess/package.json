    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-sucrase": "^5.0.1",
    "@rollup/plugin-virtual": "^3.0.1",
    "chalk": "^5.2.0",
    "combinate": "^1.1.11",
    "cosmiconfig": "^8.1.3",
    "lodash": "^4.17.21",
    "micro-memoize": "^4.0.14",
    "rollup": "^3.20.7",
    "rollup-plugin-terser": "^7.0.2",
    "yargs": "^17.7.1"
  },
  "scripts": {
    "ci": "yarn build && yarn test:all",
