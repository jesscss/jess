{
  "name": "jess-old",
  "private": true,
  "version": "1.0.8-alpha.8",
  "description": "JavaScript Enhanced Style Sheets",
  "main": "lib/index",
  "types": "lib/index",
  "bin": "./bin/cli.js",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@jesscss/parser": "workspace:1.0.8-alpha.6",
